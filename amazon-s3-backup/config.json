{
  "name": "Amazon S3 Backup Addon for Home Assistant",
  "version": "1.2.2",
  "slug": "amazon-s3-backup",
  "description": "Automatically sync your Home Assistant backups to your Amazon S3 bucket for secure and reliable storage.",
  "url": "https://github.com/watsonpaul80/hassio-addons/tree/main/amazon-s3-backup",
  "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
  "init": false,
  "stage": "stable",
  "startup": "once",
  "boot": "manual",
  "options": {
    "aws_access_key": "",
    "aws_secret_access_key": "",
    "bucket_name": "",
    "bucket_region": "eu-central-1",
    "storage_class": "STANDARD",
    "delete_local_backups": true,
    "local_backups_to_keep": 3
  },
  "schema": {
    "aws_access_key": "str",
    "aws_secret_access_key": "password",
    "bucket_name": "str",
    "bucket_region": {
      "enum": [
        "str(us-east-1)",
        "str(us-east-2)",
        "str(us-west-1)",
        "str(us-west-2)",
        "str(af-south-1)",
        "str(ap-east-1)",
        "str(ap-south-2)",
        "str(ap-southeast-3)",
        "str(ap-southeast-4)",
        "str(ap-south-1)",
        "str(ap-northeast-3)",
        "str(ap-northeast-2)",
        "str(ap-southeast-1)",
        "str(ap-southeast-2)",
        "str(ap-northeast-1)",
        "str(ca-central-1)",
        "str(eu-central-1)",
        "str(eu-west-1)",
        "str(eu-west-2)",
        "str(eu-south-1)",
        "str(eu-west-3)",
        "str(eu-south-2)",
        "str(eu-north-1)",
        "str(eu-central-2)",
        "str(me-south-1)",
        "str(me-central-1)",
        "str(sa-east-1)",
        "str(us-gov-east-1)",
        "str(us-gov-west-1)"
      ]
    },
    "storage_class": {
      "enum": [
        "str(STANDARD)",
        "str(REDUCED_REDUNDANCY)",
        "str(STANDARD_IA)",
        "str(ONEZONE_IA)",
        "str(INTELLIGENT_TIERING)",
        "str(GLACIER)",
        "str(DEEP_ARCHIVE)"
      ]
    },
    "delete_local_backups": "bool",
    "local_backups_to_keep": "int"
  },
  "map": ["backup:rw"],
  "panel_icon": "mdi:cloud-upload",
  "panel_title": "S3 Backup",
  "devices": [],
  "network": {
    "mode": "default"
  },
  "website": "https://github.com/watsonpaul80/hassio-addons/blob/main/README.md",
  "issue_tracker": "https://github.com/watsonpaul80/hassio-addons/issues",
  "source_url": "https://github.com/watsonpaul80/hassio-addons/tree/main/amazon-s3-backup"
}